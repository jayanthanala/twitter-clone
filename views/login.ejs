  <%- include("partials/header") %>
     <body class="segoe">
         <div class="topcontainer">
             <div class="topbar">
                 <i class="fa fa-twitter logocenter" aria-hidden="true"></i>
             </div>

             <div class="AppContent wrapper wrapper-signup" id="page-container">

                 <div class="page-canvas">
                     <div class="signup-wrapper">
                         <h1>
                             Welcome Back!
                         </h1>
                        
                         <form class="" action="/login" method="post">
                           <div class="t1-form signup " id="phx-signup-form">
                               <div class="textbox">


                                   <div class="prompt name">
                                       <div data-fieldname="name" class="field">
                                           <div class="sidetip">
                                               <p id="uiderror" role="alert" class="blank invalid error">Write a user id</p>
                                           </div>
                                           <input type="text" name="username" placeholder="Username" aria-required="true" maxlength="20" id="uid" class="" required>
                                       </div>
                                   </div>


                                   <div class="prompt password">
                                       <div data-fieldname="password" class="field">
                                           <div class="sidetip">
                                               <p id="passerror" role="alert" class="blank error">Please enter a password.</p>
                                           </div>
                                           <input type="password" name="password" placeholder="Password" aria-required="true" id="password" required>
                                       </div>
                                   </div>
                               </div>
                               <div class="doit">
                                   <div class="sign-up-box">
                                       <input type="submit" value="Login" id="submit_button" class="signup EdgeButton EdgeButton--primary EdgeButton--large submit">

                                   </div>
                                   <p>
                                     Don't own an account? <b><a href="/register">Register</a></b>
                                   </p>
                               </div>
                           </div>
                         </form>

                     </div>
                 </div>
             </div>
         </div>
  <%- include("partials/footer") %>
